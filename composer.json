{
  "name": "mikolfaro/symfony-apm-agent",
  "description": "Symfony APM client",
  "type": "library",
  "keywords": ["elastic", "apm", "monitoring", "performance", "symfony"],
  "license": "MIT",
  "authors": [
    {
      "name": "Mikol Faro",
      "email": "mikol.faro@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "platform": {
      "php": "7.2.2"
    },
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "require": {
    "php": "^7.2",
    "jean85/pretty-package-versions": "^1.2",
    "php-http/async-client-implementation": "^1.0",
    "symfony/config": "^4.1",
    "symfony/dependency-injection": "^4.1",
    "symfony/http-foundation": "^4.1",
    "symfony/http-kernel": "^4.1",
    "symfony/security": "^4.1",
    "symfony/stopwatch": "^4.1",
    "techdeco/elastic-apm-agent": "^0.6.0"
  },
  "require-dev": {
    "gamez/psr-testlogger": "^3.0",
    "jangregor/phpstan-prophecy": "^0.2.0",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/message": "^1.6",
    "phpstan/phpstan": "^0.10.3",
    "phpstan/phpstan-phpunit": "^0.10.0",
    "phpunit/phpunit": "^7.0",
    "psr/log-implementation": "^1.0",
    "squizlabs/php_codesniffer": "^3.3",
    "symfony/monolog-bundle": "^3.3",
    "symfony/phpunit-bridge": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "MikolFaro\\SymfonyApmAgentBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MikolFaro\\SymfonyApmAgentBundle\\Tests\\": "tests"
    }
  }
}
